<t:ItemChange>
    <t:ItemId Id="{Id}" ChangeKey="{ChangeKey}"/>
    <t:Updates>
        <t:SetItemField>
            <t:FieldURI FieldURI="message:IsRead"/>
            <t:Message>
                <t:IsRead>true</t:IsRead>
            </t:Message>
        </t:SetItemField>
    </t:Updates>
</t:ItemChange>
